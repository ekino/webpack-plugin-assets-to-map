{
  "name": "assets-to-map-webpack-plugin",
  "version": "1.2.2",
  "author": "Sebastien Picq @kheltdire",
  "description": "Append build assets to a mappable file",
  "license": "MIT",
  "main": "dist/assetsToMap.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "test": "yarn jest",
    "security": "yarn nsp check --reporter summary",
    "lint": "prettier --trailing-comma es5 --write \"src/*.js\"",
    "version": "generate-changelog && git add -A ./CHANGELOG.md"
  },
  "engines": {
    "node": ">= 4.8 < 5.0.0 || >= 5.10"
  },
  "homepage": "https://github.com/ekino/webpack-plugin-assets-to-map",
  "repository": {
    "type": "git",
    "url": "https://github.com/ekino/webpack-plugin-assets-to-map"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.40",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "generate-changelog": "^1.7.0",
    "jest": "^22.4.2",
    "nsp": "^3.2.1",
    "prettier": "1.11.1"
  }
}
