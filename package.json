      "/node_modules/",
      "/dist"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^5.1.3",
    "generate-changelog": "^1.8.0",
    "jest": "^25.2.3",
    "prettier": "1.11.1"
  }
